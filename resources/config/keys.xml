<!-- Key names are defined here: https://wiki.libsdl.org/SDL_Keycode -->
<keys>
	<key name="Left"      key="A"/>
	<key name="Right"     key="D"/>
	<key name="Up"        key="W"/>
	<key name="Down"      key="S"/>

	<key name="Jump"      key="Space"/>
	<key name="Fly"       key="X"/>
	<key name="Sneak"     key="Left Shift"/>
	<key name="Sprint"    key="Left Ctrl"/>

	<key name="Dig"       key="L"/>
	<key name="Use"       key="M"/>

	<key name="Inventory" key="E"/>
	<key name="Chat"      key="T"/>
</keys>
